{
  "name": "ncog",
  "version": "0.1.0",
  "description": "JavaScript SDK for NCOG Earth Chain RPC",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": {
    "./src/webassembly/mlkem-node.ts": "./src/webassembly/mlkem-browser.ts",
    "./dist/index.cjs.js": "./dist/index.browser.esm.js",
    "./dist/index.esm.js": "./dist/index.browser.esm.js",
    ".": "./dist/index.browser.esm.js"
  },
  "unpkg": "dist/index.umd.js",
  "jsdelivr": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ncog-Earth-Chain/ncog-sdk.git"
  },
  "keywords": [
    "ncog",
    "blockchain",
    "rpc",
    "json-rpc",
    "sdk",
    "web3",
    "ethereum",
    "postquantum",
    "mldsa87"
  ],
  "author": "developer@ncog.earth",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "rimraf dist && rollup -c --bundleConfigAsCjs && cpr src/webassembly dist/webassembly -o && cpr src/webassembly dist/ -o",
    "test": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build && npm test"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "ethereumjs-util": "^7.1.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-url": "^8.0.2",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "buffer": "^6.0.3",
    "cpr": "^3.0.1",
    "crypto-browserify": "^3.12.1",
    "eslint": "^8.0.0",
    "ethers": "^6.15.0",
    "jest": "^29.0.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.44.2",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  }
}
